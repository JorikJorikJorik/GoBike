<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_focused="false">
    <shape android:shape="rectangle">
      <corners android:radius="@dimen/blue_main_button_selector.focus.radius"/>
      <solid android:color="@color/blue_main_button_selector.focus"/>
    </shape>
  </item>
  <item android:state_enabled="false">
    <shape android:shape="rectangle">
      <corners android:radius="@dimen/blue_main_button_selector.focus.radius"/>
      <solid android:color="@color/blue_main_button_selector.enable"/>
    </shape>
  </item>
</selector>